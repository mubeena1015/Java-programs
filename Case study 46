class Task extends Thread {
    public void run() {
        for (int i = 0; i < 3; i++) System.out.println("Task: " + i);
    }
}

public class Main {
    public static void main(String[] args) {
        new Task().start();
        for (int i = 0; i < 3; i++) System.out.println("Main: " + i);
    }
}
