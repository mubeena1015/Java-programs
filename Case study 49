class Task implements Runnable {
    public void run() {
        for (int i = 0; i < 3; i++) System.out.println(Thread.currentThread().getName() + " " + i);
    }
}

public class Main {
    public static void main(String[] args) {
        Thread t1 = new Thread(new Task(), "Low");
        Thread t2 = new Thread(new Task(), "High");
        t1.setPriority(1);
        t2.setPriority(10);
        t1.start();
        t2.start();
    }
}
