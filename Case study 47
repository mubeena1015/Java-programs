class MyRunnable implements Runnable {
    public void run() {
        for (int i = 0; i < 3; i++) System.out.println("Runnable: " + i);
    }
}

public class Main {
    public static void main(String[] args) {
        Thread t = new Thread(new MyRunnable());
        t.start();
        for (int i = 0; i < 3; i++) System.out.println("Main: " + i);
    }
}
